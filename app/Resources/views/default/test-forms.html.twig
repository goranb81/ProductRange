<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Hello test</title>
    {% block stylesheets %}
        {#<link rel="stylesheet" href="{{  asset('assets/bootstrap-3.3.7-dist/css/bootstrap.min.css') }}"/>#}
        {#<link rel="stylesheet" href="{{  asset('assets/bootstrap-3.3.7-dist/css/bootstrap_superhero.min.css') }}"/>#}
        <link rel="stylesheet" href="{{  asset('assets/bootstrap-3.3.7-dist/css/bootstrap_united.min.css') }}"/>
    {% endblock %}
</head>
<body>
{% block body %}

    <div class="container">
       <form role="form" class="form-horizontal">
           <div class="form-group">
                <label for="email" class="control-label col-md-3">Email:</label>
               <div class="col-md-6">
                   <input type="email" class="form-control" name="email" id="email" placeholder="Email" />
               </div>
           </div>
           <div class="form-group">
               <label for="twitter" class="control-label col-md-3">Twitter:</label>
               <div class="col-md-6">
                   <div class="input-group">
                       <span class="input-group-addon">@</span>
                       <input type="test" class="form-control" name="twitter" id="tweitter" placeholder="Twitter" />
                   </div>
               </div>
           </div>

           <div class="form-group">
               <div class="col-md-offset-3 btn-group" data-toggle="buttons">
                   <label class="btn btn-primary active">
                       <input type="radio" name="membership-level" id="gold" checked/>Gold
                   </label>
                   <label class="btn btn-primary">
                       <input type="radio" name="membership-level" id="silver"/>Silver
                   </label>
                   <label class="btn btn-primary">
                       <input type="radio" name="membership-level" id="bronze"/>Bronze
                   </label>
               </div>
           </div>

           <div class="form-group">
                <button type="button" class="btn btn-success col-md-4 col-md-offset-3">Submit</button>
           </div>
       </form>
    </div>

{% endblock %}
{% block javascript %}
    <script src="{{ asset('assets/jQuery-3.1.1/jquery-3.1.1.min.js') }}"></script>
    <script src="{{ asset('assets/bootstrap-3.3.7-dist/js/bootstrap.min.js') }}"></script>
{% endblock %}

</body>
</html>